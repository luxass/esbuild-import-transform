{
  "name": "esbuild-plugin-import-transform",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.6.0",
  "description": "A esbuild plugin, that allows transforming imports",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/esbuild-import-transform",
  "repository": "https://github.com/luxass/esbuild-import-transform",
  "bugs": {
    "url": "https://github.com/luxass/esbuild-import-transform/issues"
  },
  "keywords": [
    "esbuild",
    "plugin",
    "transform",
    "import"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "vitest",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "esbuild": ">=0.16.17"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.2.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.2"
  }
}
